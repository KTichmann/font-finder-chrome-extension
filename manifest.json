{
  "manifest_version": 2,
  "name": "Extension Test",
  "version": "0.1",

  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "css":["popup.css"],
      "html": ["htmltest.html"],
      "js": ["content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "logo.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "activeTab",
    "tabs"
  ]

}
